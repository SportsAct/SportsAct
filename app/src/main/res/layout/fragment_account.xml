        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="37dp"
        android:layout_toEndOf="@+id/profilePic"
        android:text="I love playing basketball and football on the weekends in Bayfront Park. Let me know if you're free on the weekends!"
        tools:text="I love playing basketball and football on the weekends in Bayfront Park. Let me know if you're free on the weekends!" />


